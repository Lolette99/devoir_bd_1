Table etudiants {
matricule integer [primary key]
nom varchar [not null]
prenom varchar [not null]
email varchar [not null]
natel varchar
}

Table professeurs {
id_professeur integer [primary key]
nom varchar [not null]
prenom varchar [not null]
bureau varchar [not null]
email varchar [not null]
}

Table cours {
id_cours integer [primary key]
nom varchar [not null]
id_professeur integer [not null]
}

Table inscriptions {
id_etudiant integer [primary key]
id_cours integer [primary key]
}

Table evaluations {
  id_evaluation integer [primary key]
  date_evaluation date [not null]
  note float [not null]
  poids float [not null]
  id_etudiant integer [not null]
  id_cours integer [not null]
}

Table assistants {
  id_etudiant integer [primary key]
  id_cours integer [primary key]
  bureau varchar [not null]
}

Ref: inscriptions.id_etudiant > etudiants.matricule
Ref: inscriptions.id_cours > cours.id_cours
Ref: cours.id_professeur > professeurs.id_professeur
Ref: evaluations.id_etudiant > inscriptions.id_etudiant
Ref: evaluations.id_cours > inscriptions.id_cours
Ref: assistants.id_etudiant > etudiants.matricule
Ref: assistants.id_cours > cours.id_cours
